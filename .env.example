# Fantasy Football Dashboard - Environment Variables
# Copy this file to .env.local for local development
# Add these to Vercel Environment Variables for production

# =============================================================================
# SUPABASE (Authentication & Database)
# Get these from: https://supabase.com/dashboard/project/YOUR_PROJECT/settings/api
# =============================================================================
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# =============================================================================
# STRIPE (Payments) - Add when ready for monetization
# Get these from: https://dashboard.stripe.com/apikeys
# =============================================================================
# VITE_STRIPE_PUBLISHABLE_KEY=pk_test_xxxxx
# STRIPE_SECRET_KEY=sk_test_xxxxx (server-side only, don't prefix with VITE_)
# STRIPE_WEBHOOK_SECRET=whsec_xxxxx

# =============================================================================
# OPENAI (Optional - for AI analysis features)
# Get from: https://platform.openai.com/api-keys
# =============================================================================
# VITE_OPENAI_API_KEY=sk-xxxxx

# =============================================================================
# APP CONFIGURATION
# =============================================================================
VITE_APP_NAME=Fantasy Football Dashboard
VITE_APP_URL=https://your-domain.com

# =============================================================================
# FEATURE FLAGS (Optional)
# =============================================================================
# VITE_ENABLE_PAYMENTS=false
# VITE_ENABLE_AI_ANALYSIS=false
# VITE_MAINTENANCE_MODE=false
